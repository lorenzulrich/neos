# TYPO3.Neos:VariantMenu renders all existing node variants for possible dimension variants
prototype(TYPO3.Neos:VariantMenu) < prototype(TYPO3.Neos:Menu) {
	@class = 'TYPO3\\Neos\\TypoScript\\VariantMenuImplementation'

	templatePath = 'resource://TYPO3.Neos/Private/Templates/TypoScriptObjects/VariantMenu.html'

	renderHiddenInIndex = TRUE
	labelExpression = ${node ? node.label : ''}
	absent.attributes = TYPO3.TypoScript:Attributes {
		class = 'normal'
	}
}